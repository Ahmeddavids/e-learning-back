{
  "name": "e-learning-back",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "tsc && node dist/server.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.+(ts|js|json)\"",
    "format": "npm run prettier -- --write",
    "check:format": "npm run prettier -- --check",
    "check-types": "tsc --noEmit",
    "validate": "npm run check:format && npm run lint && npm run build",
    "pre-commit-validate": "npm-run-all --parallel check-types lint-staged",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheCurveAfrica/e-learning-back.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheCurveAfrica/e-learning-back/issues"
  },
  "homepage": "https://github.com/TheCurveAfrica/e-learning-back#readme",
  "description": "",
    "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/safe-json-stringify": "^1.1.5",
    "dotenv": "^16.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0"
  }
}
